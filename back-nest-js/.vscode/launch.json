{
    "version": "0.2.0",
    "configurations": [
      {
        "type": "node",
        "request": "launch",
        "name": "Debug NestJS",
        "runtimeExecutable": "yarn",
        "runtimeArgs": [
          "run",
          "start:debug"
        ],
        "port": 9229,
        "sourceMaps": true,
        "env": {
          "TS_NODE_PROJECT": "tsconfig.json"
        },
        "outFiles": [
          "${workspaceFolder}/dist/**/*.js"
        ]
      }
    ]
  }
  